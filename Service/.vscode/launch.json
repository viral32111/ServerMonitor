{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Collector (Debug)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "Build (Debug)",
			"program": "${workspaceFolder}/ServerMonitor/bin/Debug/net7.0/ServerMonitor.dll",
			"args": [
				"--once",
				"collector"
			],
			"cwd": "${workspaceFolder}",
			"console": "internalConsole",
			"stopAtEntry": false,
			"logging": {
				"moduleLoad": false
			}
		},
